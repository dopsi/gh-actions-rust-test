{
    "name": "GH Actions C test",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "GitHub.vscode-github-actions"
            ],
            "settings": {
                "rust-analyzer.restartServerOnConfigChange": true,
                "editor.formatOnSave": true
            }
        }
    }
}